@model Stb.Data.Models.Project

@{
    ViewData["Title"] = "删除项目";
}

<h2>@ViewData["Title"]</h2>
<br />
<h4 class="text-danger text-center">确定删除项目？</h4>
<div>
    <h4>项目信息</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayNameFor(model => model.Name)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Client)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Client)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.Contract)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Contract)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.ContractNo)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ContractNo)
        </dd>
        <dt>
            @Html.DisplayNameFor(model => model.ContractAmount)
        </dt>
        <dd>
            @Html.DisplayFor(model => model.ContractAmount)
        </dd>
        <dt>
            <pre>@Html.DisplayNameFor(model => model.Description)</pre>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.Description)
        </dd>
       
    </dl>
    
    <form asp-action="Delete">
        <div class="form-actions no-color">
            <input type="submit" value="删除" class="btn btn-default" /> |
            <a asp-action="Index">返回</a>
        </div>
    </form>
</div>
